{
	"extends": [
		"react-app",
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:import/recommended",
		"prettier"
	],
	"plugins": ["react"],
	"rules": {
		"indent": ["off", 4],
		"linebreak-style": ["off", "unix"],
		"quotes": ["off", "single"],
		"semi": ["off", "always"],
		"array-bracket-spacing": ["off", "always"],
		"brace-style": ["off", "1tbs"],
		"camelcase": "off",
		"comma-dangle": ["off", "always-multiline"],
		"comma-spacing": "off",
		"comma-style": "off",
		"computed-property-spacing": ["off", "always"],
		"constructor-super": "off",
		"consistent-return": "off",
		"curly": "off",
		"dot-notation": "off",
		"eqeqeq": ["off", "allow-null"],
		"eol-last": "off",
		"func-call-spacing": "off",
		"jsx-quotes": ["off", "prefer-double"],
		"key-spacing": "off",
		"keyword-spacing": "off",
		"max-len": ["off", { "code": 105 }],
		"new-cap": ["off", { "capIsNew": false, "newIsCap": true }],
		"no-cond-assign": "off",
		"no-const-assign": "off",
		"no-console": "off",
		"no-debugger": "off",
		"no-dupe-args": "off",
		"no-dupe-keys": "off",
		"no-duplicate-case": "off",
		"no-duplicate-imports": "off",
		"no-else-return": "off",
		"no-empty": ["off", { "allowEmptyCatch": true }],
		"no-extra-semi": "off",
		"no-fallthrough": "off",
		"no-lonely-if": "off",
		"no-mixed-requires": "off",
		"no-mixed-spaces-and-tabs": "off",
		"no-multiple-empty-lines": ["off", { "max": 1 }],
		"no-multi-spaces": "off",
		"no-negated-in-lhs": "off",
		"no-nested-ternary": "off",
		"no-new": "off",
		"no-process-exit": "off",
		"no-prototype-builtins": "off",
		"no-redeclare": "off",
		"no-shadow": "off",
		"no-spaced-func": "off",
		"no-trailing-spaces": "off",
		"no-undef": "off",
		"no-underscore-dangle": "off",
		"no-unreachable": "off",
		"no-unused-vars": "off",
		"no-var": "off",
		"object-curly-spacing": ["off", "never"],
		"one-var": "off",
		"operator-linebreak": [
			"off",
			"after",
			{
				"overrides": {
					"?": "before",
					":": "before"
				}
			}
		],
		"padded-blocks": ["off", "never"],
		"prefer-const": "off",
		"quote-props": ["off", "as-needed"],
		"quotes": ["off", "single", "avoid-escape"],
		"semi": "off",
		"semi-spacing": "off",
		"space-before-blocks": ["off", "always"],
		"space-before-function-paren": [
			"off",
			{
				"anonymous": "never",
				"asyncArrow": "always",
				"named": "never"
			}
		],
		"space-in-parens": ["off", "never"],
		"space-infix-ops": ["off", { "int32Hint": false }],
		"space-unary-ops": [
			"off",
			{
				"overrides": {
					"!": true
				}
			}
		],
		"react/jsx-curly-spacing": [0, "always"],
		"react/jsx-no-duplicate-props": 0,
		"react/jsx-no-target-blank": 0,
		"react/jsx-no-undef": 0,
		"react/jsx-tag-spacing": 0,
		"react/jsx-uses-react": 0,
		"react/jsx-uses-vars": 0,
		"react/no-danger": 0,
		"react/no-deprecated": 0,
		"react/no-did-mount-set-state": 0,
		"react/no-did-update-set-state": 0,
		"react/no-is-mounted": 0,
		"react/no-string-refs": 0,
		"react/prefer-es6-class": 0,
		"react/react-in-jsx-scope": 0,
		"react-hooks/rules-of-hooks": 0,
		"react-hooks/exhaustive-deps": 0,
		"import/order": [
			"off",
			{
				"groups": ["builtin", "external", "parent", "sibling", "index"],
				"newlines-between": "always",
				"alphabetize": {
					"order": "asc",
					"caseInsensitive": true
				}
			}
		]
	},
	"settings": {
		"import/resolver": {
			"node": {
				"paths": ["src"],
				"extensions": [".js", ".jsx"]
			}
		}
	}
}
